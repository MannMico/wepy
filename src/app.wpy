<script>
import wepy from 'wepy'
import 'wepy-async-function'
import Interface from './interface'
import Util from './util'
// import { setStore } from 'wepy-redux'

export default class MyAPP extends wepy.app {
  config = {
    pages: [
      'index/index',  //  首页
      'share/share',   //  作品分享页
      'login/login',  //  画图页
      'draw/draw',  //  画图页
      'drawlist/drawlist',  //  作品列表页
      'picsave/picsave',  //  图片模板页
      'rankinglist/rankinglist',  //  排行榜
      'drawdetail/drawdetail',  //  作品详情页
      'drafts/drafts',  //  草稿箱
      'mydraw/mydraw'  //  个人中心页
    ],
    subPackages: [
      {
        root: 'packageA',
        pages: [
          'detail/detail' //  详情页
        ]
      }
    ],
    window: {
      // enablePullDownRefresh: true,
      backgroundTextStyle: 'dark',
      navigationBarBackgroundColor: '#1a1a1a',
      navigationBarTitleText: 'WeChat',
      navigationBarTextStyle: '#fff'
    }
  }
  globalData = {
    saveImage: '',
    loginStatus: 0,
    toHref: '',
    loading: 0,
    sex: 1
  }
  constructor () {
        // this is not allowed before super()
    super()
        // 拦截request请求
    this.intercept('request', {
            // 发出请求时的回调函数
      config (p) {
        p.header['X-Nonce'] = Interface.getRandomStr()
        p.header['X-Time'] = Interface.getUnixtime()
        p.header['X-Sign'] = Interface.getSignData(p, 'nTBMFzvVKVL2keCsrzaNeUUC')
        // p.header['X-Sign'] = Interface.getSignData(p, 'lkker')
        p.header['Authorization'] = 'Bearer ' + wx.getStorageSync('api_token')
        // debugger
                // 对所有request请求中的OBJECT参数对象统一附加时间戳属性
        console.log('config request: ', p)
              // 必须返回OBJECT参数对象，否则无法发送请求到服务端
        return p
      }
    })
  }
  onLaunch () {
    var self = this
    wx.checkSession({
      success: function() {
        Util.requestAjax({
          url: '/v1/common/login/status',
          method: 'get',
          data: {},
          success (data) {
            if (data.data.code == 300000) {
              self.globalData.loginStatus = 1
            } else {
              self.globalData.loginStatus = 0
            }
          }
        })
      },
      fail: function() {
        self.globalData.loginStatus = 0
      }
    })
  }
}

// console.log('process.env.NODE_ENV =>' + process.env.NODE_ENV)
</script>
